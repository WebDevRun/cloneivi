{
  "extends": [
    ["next/babel", "next/core-web-vitals"],
    "plugin:storybook/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        },
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}
